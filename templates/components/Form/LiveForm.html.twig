<div {{ attributes }}>
	{% set defaultAttr = {
			'data-action': 'live#action:prevent',
			'data-live-action-param': 'save',
			'novalidate': 'novalidate'
	} %}


	{% set extraAttr = get_class_name(form.vars.value) == 'teletravailform' 
    ? typeOfCollaborator == 'manager' ? {
        'data-controller': 'manager--teleworking',
        'data-manager--teleworking-target': 'form'
      }
    : {
		'data-controller':'rh--teleworking', 
		'data-rh--teleworking-target':'form'
		}
	: {}
	%}

	{% set formAttr = defaultAttr|merge(extraAttr) %}

	{{ form_start(form, {'attr': formAttr}) }}

	{% if typeOfCollaborator == 'rh' %}
		{% include "rh/teletravail_form/_form_content.html.twig" with { 'form': form } %}
	{% else %}
	<pre>
		{{ dump('formulaire manager') }}
	</pre>
		{% include "manager/teletravail_form/_form_content.html.twig" with { 'form': form } %}
	{% endif %}


	{{ form_end(form) }}
